void main()
{
sum(2,5);
subtract(10,4);
multiply(5,6);
divide(54,4);
}

void sum(int x,int y) => print('Sum is ${x + y}');
void subtract(int x, int y) => print('Difference is ${x - y}');
void multiply(int x, int y) => print('Multiplication is ${x * y}');
void divide(int x, int y) => print('Division is ${x / y}');
